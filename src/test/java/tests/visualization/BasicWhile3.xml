<?xml version="1.0" encoding="ISO-8859-1" ?>
<!DOCTYPE configuration PUBLIC "-//DEEPSEA//COASTAL configuration//EN" "https://deepseaplatform.github.io/coastal/coastal.dtd">
<configuration>
	<coastal>
		<target>
			<main>tests.visualization.BasicWhile3</main>
			<instrument>tests.visualization</instrument>
			<trigger>tests.visualization.BasicWhile3.test(X: int)</trigger>
		</target>
		
		<settings>
			<echo-output>false</echo-output>
			<draw-paths>true</draw-paths>
		</settings>
		<strategies>
			<strategy>za.ac.sun.cs.coastal.strategy.pathbased.DepthFirstFactory</strategy>
		</strategies>
		<observers>
			<observer>ceo.GUIFactoryFX</observer>
		</observers>
		
		
	</coastal>
</configuration>

